{
    "name": "gatsby-starter-hello-world",
    "private": true,
    "description": "A simplified bare-bones starter for Gatsby",
    "version": "0.1.0",
    "license": "0BSD",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "start": "npm run develop",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
    },
    "dependencies": {
        "gatsby": "^3.0.1",
        "gatsby-plugin-google-fonts": "^1.0.1",
        "gatsby-plugin-i18n": "^1.0.1",
        "gatsby-plugin-image": "^1.0.0",
        "gatsby-plugin-postcss": "^4.0.0",
        "gatsby-plugin-react-helmet": "^3.3.13",
        "gatsby-plugin-sharp": "^3.0.0",
        "gatsby-source-storyblok": "git@github.com:storyblok/gatsby-source-storyblok.git",
        "gatsby-transformer-sharp": "^3.0.0",
        "postcss": "^8.2.6",
        "postcss-preset-env": "^6.7.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-helmet": "^6.1.0",
        "storyblok-js-client": "^3.3.1",
        "storyblok-react": "^0.1.2",
        "storyblok-rich-text-react-renderer": "^2.1.0"
    },
    "devDependencies": {
